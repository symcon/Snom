{
    "elements": [
        {
            "type": "Label",
            "caption": "Configure Snom IP phone",
            "name": "InstanceLabel"
        },
        {
            "type": "Select",
            "name": "Protocol",
            "caption": "Protocol",
            "options": [
                {
                    "caption": "HTTP",
                    "value": "http"
                },
                {
                    "caption": "HTTPS",
                    "value": "https"
                }
            ]
        },
        {
            "type": "RowLayout",
            "items": [
                {
                    "type": "ValidationTextBox",
                    "caption": "IP address",
                    "name": "PhoneIP",
                    "validate": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?).){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$"
                },
                {
                    "type": "Button",
                    "caption": "Reachable?",
                    "onClick": "echo SNMD_PingPhone($id, $PhoneIP);"
                },
                {
                    "type": "ValidationTextBox",
                    "caption": "Phone WUI HTTP username",
                    "name": "Username",
                    "visible": false
                },
                {
                    "type": "PasswordTextBox",
                    "caption": "Phone WUI HTTP password",
                    "name": "Password",
                    "visible": false
                },
                {
                    "type": "Label",
                    "caption": "No Snom phone with this IP address",
                    "name": "NotSnomPhone",
                    "visible": false
                }
            ]
        },
        {
            "type": "ValidationTextBox",
            "caption": "MAC address",
            "name": "PhoneMac",
            "enabled": false
        },
        {
            "type": "ValidationTextBox",
            "caption": "Phone model",
            "name": "PhoneModel",
            "enabled": false
        },
        {
            "type": "ValidationTextBox",
            "caption": "Local IP address",
            "name": "LocalIP",
            "validate": "^(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?).){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$",
            "enabled": false
        },
        {
            "type": "Button",
            "caption": "See current function keys settings",
            "name": "PhoneWuiLink",
            "link": true,
            "onClick": "echo 'http://' . $PhoneIP . '/fkey.htm';"
        },
        {
            "type": "List",
            "caption": "Function keys settings",
            "name": "FkeysSettings",
            "add": true,
            "delete": true,
            "columns": [
                {
                    "caption": "Function key",
                    "name": "FkeyNo",
                    "width": "200px",
                    "align": "center",
                    "add": 0,
                    "edit": {
                        "type": "Select",
                        "options": []
                    }
                },
                {
                    "caption": "Label",
                    "name": "FkeyLabel",
                    "width": "150px",
                    "align": "center",
                    "add": "",
                    "edit": {
                        "type": "ValidationTextBox"
                    }
                },
                {
                    "caption": "Color for status on",
                    "name": "FkeyColorOn",
                    "width": "100px",
                    "align": "center",
                    "add": "none",
                    "edit": {
                        "type": "Select",
                        "options": [
                            {
                                "caption": "Green",
                                "value": "green"
                            },
                            {
                                "caption": "Red",
                                "value": "red"
                            },
                            {
                                "caption": "Yellow",
                                "value": "yellow"
                            },
                            {
                                "caption": "None",
                                "value": "none"
                            }
                        ]
                    }
                },
                {
                    "caption": "Color for status off",
                    "name": "FkeyColorOff",
                    "width": "100px",
                    "align": "center",
                    "add": "none",
                    "edit": {
                        "type": "Select",
                        "options": [
                            {
                                "caption": "Green",
                                "value": "green"
                            },
                            {
                                "caption": "Red",
                                "value": "red"
                            },
                            {
                                "caption": "Yellow",
                                "value": "yellow"
                            },
                            {
                                "caption": "None",
                                "value": "none"
                            }
                        ]
                    }
                },
                {
                    "caption": "Functionality",
                    "name": "Functionality",
                    "width": "100px",
                    "visible": true,
                    "add": 3,
                    "edit": {
                        "type": "Select",
                        "options": [
                            {
                                "caption": "Display status",
                                "value": 1
                            },
                            {
                                "caption": "Update status LED",
                                "value": 2
                            },
                            {
                                "caption": "Trigger action and update status LED",
                                "value": 3
                            }
                        ]
                    }
                },
                {
                    "caption": "Action variable",
                    "name": "ActionVariableId",
                    "width": "300px",
                    "align": "center",
                    "add": 0,
                    "edit": {
                        "type": "SelectVariable"
                    }
                },
                {
                    "caption": "Action",
                    "name": "ActionValue",
                    "width": "100px",
                    "align": "center",
                    "add": 0,
                    "edit": {
                        "type": "SelectAction"
                    }
                },
                {
                    "caption": "Use other variable for status LED",
                    "name": "StatusVariable",
                    "width": "100px",
                    "visible": false,
                    "add": false,
                    "edit": {
                        "type": "CheckBox"
                    }
                },
                {
                    "caption": "Status variable",
                    "name": "StatusVariableId",
                    "width": "auto",
                    "align": "left",
                    "add": 0,
                    "edit": {
                        "type": "SelectVariable"
                    }
                }
            ],
            "values": [],
            "form": [
                {
                    "type": "Select",
                    "name": "FkeyNo",
                    "caption": "Function key",
                    "options": []
                },
                {
                    "type": "ValidationTextBox",
                    "name": "FkeyLabel",
                    "caption": "Label"
                },
                {
                    "type": "Select",
                    "name": "FkeyColorOn",
                    "caption": "Color for status on",
                    "options": [
                        {
                            "caption": "Green",
                            "value": "green"
                        },
                        {
                            "caption": "Red",
                            "value": "red"
                        },
                        {
                            "caption": "Yellow",
                            "value": "yellow"
                        },
                        {
                            "caption": "None",
                            "value": "none"
                        }
                    ]
                },
                {
                    "type": "Select",
                    "name": "FkeyColorOff",
                    "caption": "Color for status off",
                    "options": [
                        {
                            "caption": "Green",
                            "value": "green"
                        },
                        {
                            "caption": "Red",
                            "value": "red"
                        },
                        {
                            "caption": "Yellow",
                            "value": "yellow"
                        },
                        {
                            "caption": "None",
                            "value": "none"
                        }
                    ]
                },
                {
                    "type": "Select",
                    "name": "Functionality",
                    "caption": "Functionality",
                    "options": [
                        {
                            "caption": "Display status",
                            "value": 1
                        },
                        {
                            "caption": "Update status LED",
                            "value": 2
                        },
                        {
                            "caption": "Trigger action and update status LED",
                            "value": 3
                        }
                    ],
                    "onChange": "SNMD_setFkeyFunctionality($id, $Functionality);"
                },
                {
                    "type": "SelectVariable",
                    "name": "ActionVariableId",
                    "caption": "Action variable",
                    "visible": false
                },
                {
                    "type": "SelectAction",
                    "name": "ActionValue",
                    "caption": "Action",
                    "onChange": "SNMD_SetVariablesIds($id, $ActionValue, false);"
                },
                {
                    "type": "CheckBox",
                    "name": "StatusVariable",
                    "caption": "Use other variable for status LED",
                    "onChange": "SNMD_SetVariablesIds($id, $ActionValue, $StatusVariable);"
                },
                {
                    "type": "SelectVariable",
                    "name": "StatusVariableId",
                    "caption": "Status variable",
                    "visible": false
                }
            ],
            "onEdit": [
                "echo 'Are you sure?\n",
                "Applying the changes, will override the edited function(s) key(s) settings in your phone.\n",
                "For seeing the current fkey settings, press the button \"See current function keys settings\"';"
            ],
            "onAdd": [
                "echo 'Are you sure?\n",
                "Applying the changes, will override the edited function(s) key(s) settings in your phone.\n",
                "For seeing the current fkey settings, press the button \"See current function keys settings\"';"
            ]
        },
